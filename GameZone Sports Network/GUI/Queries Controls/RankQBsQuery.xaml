<UserControl x:Class="GUI.RankQBsQuery"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GUI"
             mc:Ignorable="d" 
             d:DesignHeight="250" d:DesignWidth="300">
    <Border BorderBrush="#2c0483" BorderThickness="5">
        <Grid Background="LightGray">
        <Grid.RowDefinitions>
            <RowDefinition Height=".15*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock FontSize="18" FontWeight="Black">Ranking QBs - Passing TDs</TextBlock>
        <ListBox FontSize="15" FontWeight="Bold" Background="LightGray" Grid.Row="1" x:Name="rankingListBox"></ListBox>
    </Grid>
    </Border>
</UserControl>
